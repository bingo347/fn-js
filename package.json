  "bugs": {
    "url": "https://github.com/bingo347/fn-js/issues"
  },
  "homepage": "https://github.com/bingo347/fn-js#readme",
  "devDependencies": {
    "@babel/generator": "^7.4.4",
    "@babel/parser": "^7.4.5",
    "@babel/traverse": "^7.4.5",
    "@babel/types": "^7.4.4",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "ava": "^2.1.0",
    "esm": "^3.2.25",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  }
