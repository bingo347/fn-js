{
  "name": "@bingo347/fn",
  "version": "0.3.1",
  "description": "",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "test": "ava",
    "coverage": "nyc ava",
    "coverage:report": "nyc --require ts-node/register report --reporter=html",
    "lint": "tslint -p .",
    "compile": "tsc -p .",
    "clear": "rm -rf ./dist",
    "indexes": "echo TODO",
    "package": "echo TODO",
    "docs": "echo TODO",
    "build": "run-s compile",
    "prebuild": "run-s lint test clear",
    "postbuild": "run-s docs indexes package",
    "pub": "npm publish dist/package --access=public",
    "postversion": "run-s build pub"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true
  },
  "ava": {
    "compileEnhancements": false,
    "verbose": true,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bingo347/fn-js.git"
  },
  "keywords": [
